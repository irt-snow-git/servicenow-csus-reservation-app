        <element choice="3" default="subject" label="Type" max_length="40" name="type" type="string">
            <choice>
                <element label="Subject" value="subject"/>
            </choice>
        </element>
        <index name="index">
            <element name="email_template"/>
        </index>
    </element>
</database>
